		Socket socket = null;
		
		
		Scanner sc = new Scanner(System.in);
		
		try {} catch() {} finally {
			try {
				if(din != null) { din.close();}
				if(dout != null) { dout.close();}
				if(clientSocket != null) { clientSocket.close();}
